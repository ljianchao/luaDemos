{
	"version": "0.2.0",
	"configurations": [
		{
            // Name of configuration; appears in the lauch configuartion drop down menu.
			"name": "Launch Lua",
            // Type of configuration.Posssible values: "node", "mono"
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}"
			],
			"stopOnEntry": false,
			"sourceMaps": true,
			"outDir": "out",
			"preLaunchTask": "npm"
		}
	]
}